  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run lib && typedoc --out dist/docs --target es6 --theme minimal --mode file libs",
    "build:elem": "ng build --aot --prod --output-hashing=none --app 2 && gulp",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:stats": "ng build --prod --preserve-symlink --stats-json",
    "bundle-report": "npm run build:stats && webpack-bundle-analyzer dist/stats.json",
    "dist": "rimraf dist && gulp build",
    "package-sanitize": "node ./scripts/package-sanitize.js",
    "lib": "npm run package-sanitize && ng-packagr -p ./libs/ng-package.json && npm run copy:styles",
    "release": "ng build --prod && NODE_ENV='production' node s3-upload.js",
    "copy:styles": "copyfiles -u 2 libs/styles/**/* ../ht-angular/src/styles",
    "doc": "typedoc -f --out doc libs/ht-angular",
    "version": "npm run lib && ./scripts/build.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
