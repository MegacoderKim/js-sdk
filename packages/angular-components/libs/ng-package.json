{
  "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../ht-angular",
  "whitelistedNonPeerDependencies": [
    "bulma",
    "date-fns",
    "font-awesome",
    "ht-api",
    "ht-client",
    "ht-maps",
    "ht-data",
    "ht-models",
    "ht-utility",
    "jstz",
    "leaflet",
    "leaflet-ant-path",
    "leaflet.heat",
    "leaflet.markercluster",
    "popper.js",
    "time-aware-polyline",
    "underscore"
  ],
  "lib": {
    "languageLevel": [ "dom", "es2017" ],
    "entryFile": "./public_api.ts",
    "umdModuleIds": {
      "underscore": "_",
      "moment-mini": "moment",
      "ht-models": "htModels",
      "ht-utility": "htUtility",
      "ht-data": "htData",
      "ht-maps": "htMaps",
      "ht-client": "htClient",
      "time-aware-polyline": "timeAwarePolyline",
      "date-fns": "dateFns",
      "leaflet": "L",
      "rxjs/operators": "Rx.operators"
    }
  }
}
