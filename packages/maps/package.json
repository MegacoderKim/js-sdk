{
  "name": "ht-maps",
  "version": "0.1.1-0",
  "description": "hypertrack maps",
  "main": "dist/ht-maps_browser.js",
  "module": "dist/ht-maps.es5.js",
  "typings": "dist/types/ht-maps.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run lib && typedoc --out dist/docs --target es6 --theme minimal --mode file src",
    "build:webpack": "rimraf dist && webpack --config webpack.config.js",
    "wbuild": "webpack -w --config webpack.config.bundle.js",
    "build:style": "webpack --config webpack.config.js",
    "lib": "tsc && tsc --module commonjs --outDir dist/lib && rollup -c rollup.config.ts && npm run build:style",
    "start": "tsc -w & rollup -c rollup.config.ts -w",
    "prepack": "npm run lib"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/googlemaps": "^3.26.14",
    "@types/leaflet": "^1.2.5",
    "@types/leaflet.heat": "^0.2.0",
    "@types/leaflet.markercluster": "^1.0.2",
    "@types/node": "^8.0.4",
    "ht-data": "^0.0.5-beta",
    "ht-models": "^0.0.13",
    "ht-utility": "^0.1.0",
    "leaflet": "^1.2.0",
    "leaflet.markercluster": "^1.2.0",
    "rollup": "^0.53.1",
    "underscore": "^1.8.x"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "ht-data": "^0.1.1-0",
    "leaflet.heat": "^0.2.0",
    "time-aware-polyline": "^0.0.2-0"
  },
  "peerDependencies": {
    "ht-data": ">=0.0.20",
    "ht-models": "^0.0.x",
    "ht-utility": "^0.1.0",
    "rxjs": "^5.5.2",
    "underscore": "^1.8.x"
  },
  "false": {}
}
