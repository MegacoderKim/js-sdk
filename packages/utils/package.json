{
  "name": "ht-utility",
  "version": "0.0.4-beta",
  "description": "HyperTrack utils library",
  "main": "dist/ht-utility_browser.js",
  "module": "dist/ht-utility.es5.js",
  "typings": "dist/types/ht-utility.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
